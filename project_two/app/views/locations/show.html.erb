
<center><h1><%= @location.name %></h1>


   <div class="hexagons">
 <% @location.contents.each do |content| %>
 <% content_url = content.content_url %>
 <% content_image = content.image_url %>
 <a href=<%= content_url %> style="background-image:url('<%= content_image %>')"></a>
<% end %>
</div>

<section class= "tweets">


  <ul>
    <img src = https://encrypted-tbn1.gstatic.com/images?q=tbn:ANd9GcS_7OGIwozJddCsdgSnpTWt-HWdKbCviu-CF4m3iM5okFMo-_tUUQ >

    <% Tweet.breaking(@location).each do |tweet|%>
      <div> ***<%=  tweet.text %> **</div >
    <% end %>
  </ul>
</section>

  <section class="insta">

<ul>
  <img src = data:image/jpeg;base64, >

  <% Insta.location_tag(@location).each do |instagram| %>
    <img src = "<%= instagram.images["thumbnail"]["url"] %>">


    <% end %>

  </ul>
</section>
